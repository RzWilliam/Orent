{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 3em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; text-align: center}
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    img{max-width: 200px}
</style>

<div class="example-wrapper">
    <img src="https://www.orent.com/build/locest-bootstrap-theme/images/logo/logoo.6c5fb759.svg" alt="Logo Orent">
    {{ form_start(form) }}
    {{ form_end(form) }}
    {% if SousSousFamille is defined %}
        <p>Sélectionné : {{ SousSousFamille.name }}</p>
        <p>Famille : {{ SousSousFamille.sousfamille.famille }}</p>
        <p>Sous Famille : {{ SousSousFamille.sousfamille }}</p>
    {% endif %}
</div>
{% endblock %}
